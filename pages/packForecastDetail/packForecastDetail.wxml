<!--pages/packForecastDetail/packForecastDetail.wxml-->
    <van-cell-group>
 <van-cell title="快递名称" value="{{packDetail.expressCompany }}" />
  <van-cell title="快递单号" value="{{packDetail.hawbNo}}" />
   <van-cell title="物品名称" value="{{packDetail.productName}}" />
    <van-cell title="物品品牌" value="{{packDetail.brand}}" />
     <van-cell title="物品价值" value="{{packDetail.svalue}}" />
      <van-cell title="物品属性" value="{{packDetail.goodsType}}" />
       <van-cell title="商品数量" value="{{packDetail.pcs}}" />
         <van-cell title="商品重量" value="{{packDetail.cwt}}" />
        <van-cell title="商品备注" value="{{packDetail.remark}}" />
        <van-cell wx:if="{{packDetail.isUnpack == 0}}" title="是否拆包" value="是" />
        <van-cell wx:if="{{packDetail.isUnpack == 1}}" title="是否拆包" value="否" />
    </van-cell-group>

